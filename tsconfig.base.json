{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "preserve",
    "esModuleInterop": true,
    "noEmit": true,
    "allowJs": true,
    "isolatedModules": true,
    "incremental": true,
    "strict": true
  }
}
