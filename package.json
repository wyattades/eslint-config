{
  "name": "@wyattades/eslint-config",
  "type": "module",
  "version": "0.20.0",
  "description": "ESLint configuration for TypeScript projects",
  "keywords": [
    "typescript",
    "eslint",
    "eslintconfig",
    "jest",
    "airbnb",
    "react.js"
  ],
  "homepage": "https://github.com/wyattades/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/wyattades/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyattades/eslint-config.git"
  },
  "license": "MIT",
  "author": "Wyatt Ades <me@wyattades.com>",
  "main": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.mjs"
    },
    "./import-order": {
      "import": "./import-order.mjs",
      "require": "./import-order.mjs"
    }
  },
  "files": [
    "index.mjs",
    "import-order.mjs"
  ],
  "scripts": {
    "test": "cd test && node ./runTests.mjs"
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.3",
    "chalk": "^5.4.1",
    "eslint": "^9.17.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "3.4.2",
    "react": "^19.0.0",
    "typescript": "~5.7.2"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "jest": ">=26",
    "prettier": ">=3",
    "react": ">=16",
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.15.3"
